# Environment Variables Template
# Copy this file to .env.local for development
# SECURITY WARNING: Never commit real credentials to source control

# API Configuration
API_BASE_URL=https://your-api-gateway.execute-api.region.amazonaws.com/stage
WEBSOCKET_URL=wss://your-websocket-api.execute-api.region.amazonaws.com/stage
API_TIMEOUT=15000
USE_MOCK_API=true

# AWS Configuration - REPLACE WITH YOUR VALUES
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=000000000000
AWS_COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
AWS_COGNITO_CLIENT_ID=XXXXXXXXXXXXXXXXXXXXXXXXX
AWS_COGNITO_IDENTITY_POOL_ID=us-east-1:XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX

# S3 Buckets
S3_GAMES_BUCKET=your-games-bucket
S3_UPLOADS_BUCKET=your-uploads-bucket
S3_ANALYTICS_BUCKET=your-analytics-bucket

# DynamoDB Tables
DYNAMODB_GAMES_TABLE=your-games-table
DYNAMODB_USERS_TABLE=your-users-table

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_WEBSOCKET=true
ENABLE_OFFLINE_MODE=true
ENABLE_PERFORMANCE_MONITORING=true
FEATURE_BIOMETRIC_AUTH=true
FEATURE_SOCIAL_LOGIN=true
FEATURE_AUTH_FALLBACK=false

# Cache Configuration
CACHE_DEFAULT_TTL=600000
CACHE_MAX_SIZE=209715200
CACHE_ENABLE_PERSISTENCE=true

# Performance Configuration
IMAGE_QUALITY=0.9
MAX_CONCURRENT_REQUESTS=6
REQUEST_RETRY_COUNT=2
REQUEST_RETRY_DELAY=3000

# Debug Configuration
DEBUG_MODE=false
LOG_LEVEL=error
ENABLE_NETWORK_INSPECTOR=false
ENABLE_REDUX_DEVTOOLS=false

# Third Party Services
# Sentry Error Tracking (Optional - leave empty to disable)
# Get your DSN from https://sentry.io/settings/projects/
# Format: https://XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX@sentry.io/XXXXXXX
REACT_APP_SENTRY_DSN=
EXPO_PUBLIC_SENTRY_DSN=
SENTRY_AUTH_TOKEN=
MIXPANEL_TOKEN=
AMPLITUDE_API_KEY=

# App Configuration
APP_VERSION=1.0.0
BUILD_NUMBER=1
ENVIRONMENT=production

# EAS Configuration
EAS_PROJECT_ID=your-eas-project-id